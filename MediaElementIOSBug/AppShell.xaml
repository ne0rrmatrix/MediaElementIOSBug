<?xml version="1.0" encoding="UTF-8" ?>
<Shell
	x:Class="MediaElementIOSBug.AppShell"
	xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
	xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
	xmlns:views="clr-namespace:MediaElementIOSBug.Views"
	xmlns:local="clr-namespace:MediaElementIOSBug">

    <ShellContent
		Shell.NavBarIsVisible="true"
		Title="Main"
		Icon="iconblank.png"
		ContentTemplate="{DataTemplate views:MainPage}"
		Route="MainPage" />
    <ShellContent
		Shell.NavBarIsVisible="true"
		Title="Seek"
		Icon="iconblank.png"
		ContentTemplate="{DataTemplate views:SeekPage}"
		Route="SeekPage" />
</Shell>
